const mysql = jest.fn();
const Knex = require('knex');
const fp = require('fastify-plugin');

Knex.knex = jest.mock();

const knexPlugin = async (fastify, options) => {
    const knext = { 
        raw: jest.fn(),
        destroy: jest.fn(),
    }

    fastify.decorate('knex', knex);
}

jest.mock('./src/plugins/knex-plugin', (() => {
  return knexPlugin;
}));

